// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n = 5, i , j;
    for(i = 1; i <= n; i++){
        for(j = 1; j <= n; j++){
            if(i%2==0){
                printf("#");
            }else{
                printf("*");
            }
        }
        printf("\n");
    }

    return 0;
}
