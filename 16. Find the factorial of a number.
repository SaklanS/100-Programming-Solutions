// Online C compiler to run C program online
#include <stdio.h>

int main() {
    
    int nmbr,i,fact = 1;
    
    printf("Please give me the  Number: ");
    scanf("%d",&nmbr);
    
    for(i = 1; i <= nmbr; i++){
        fact *= i;
    }
    printf("The factrorial Number is: %d",fact);
    
    return 0;
}
