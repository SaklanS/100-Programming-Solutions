// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int FirstNumber,SecondNumber;
    
    printf("Give me the value of First Number: ");
    scanf("%d",&FirstNumber);
    
    printf("Give me the value of Second Number: ");
    scanf("%d",&SecondNumber);
    
    printf("The of This Two number is: %d",FirstNumber + SecondNumber);

    return 0;
}
