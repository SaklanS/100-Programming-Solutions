// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n= 5, i,j;
    

    for(i = 1; i <= n; i++){
        for(j = 1; j < i; j++){
            printf(" ");
        }
        for(int k = i ; k <= n; k++){
            printf("*");
        }
        for(j = i; j <=n; j++){
            printf("*");
        }
        printf("\n");
    }
  

    for(i = 1; i <= n; i++){
        for(j = i; j < n; j++){
            printf(" ");
        }
        for(int k = 1 ; k <= i; k++){
            printf("*");
        }
        for(int l = 1; l <= i; l++){
            printf("*");
        }
        printf("\n");
    }
        

    return 0;
}
