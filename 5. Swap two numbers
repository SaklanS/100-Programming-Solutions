#include <stdio.h>

int main() {
    int FirstNumber,SecondNumber, Product;
    
    printf("Give me the value of First Number: ");
    scanf("%d",&FirstNumber);
    
    printf("Give me the value of Second Number: ");
    scanf("%d",&SecondNumber);
    
    int SwapFirstNumber = SecondNumber;
    int SwapSecondNumber = FirstNumber;
    
    printf("The Swap of First Number is : %d \n",SwapFirstNumber);
    
     printf("The Swap of Second Number is : %d \n",SwapSecondNumber);

