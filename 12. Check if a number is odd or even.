// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int number;
    
    printf("Enter the Number : ");
    scanf("%d",&number);
    
    
    if(number % 2 == 0){
        printf("The Number is Even: %d",number);
    }else{
        printf("The number is Odd: %d",number);
    }

    return 0;
}
