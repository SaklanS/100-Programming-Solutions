// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int starNumber;
    int i,j;
    
    //Asking Input of star number
    printf("Give the number of star : ");
    
    scanf("%d",&starNumber);
    
    for( i = 1; i <= starNumber; i++){
        for( j = 1; j <= i; j++){
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
